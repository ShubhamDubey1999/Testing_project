@model IEnumerable<Anurag.Models.User>

<h1>Users List</h1>
<a class="btn btn-primary float-end" asp-controller="Home" asp-action="Index">Add New User</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">S.No.</th>
            <th scope="col">Username</th>
            <th scope="col">Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>1</td>
                <td>@i.Username</td>
                <td>@i.Password</td>
            </tr>
        }
    </tbody>
</table>